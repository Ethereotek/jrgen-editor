{
  "name": "jrgen",
  "version": "1.0.0",
  "description": "Generates docs, tests, clients and servers for json-rpc apis.",
  "keywords": [
    "generator",
    "json-rpc",
    "api",
    "doc",
    "test",
    "client",
    "server",
    "editor"
  ],
  "author": "Corbin White <white.corbin91@gmail.com>",
  "license": "MIT",
  "bin": "./src/jrgen.js",
  "bugs": "https://github.com/Ethereotek/jrgen-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ethereotek/jrgen-editor.git"
  },
  "scripts": {
    "build:examples": "npm run clear:examples && node ./scripts/create-examples.js",
    "clear:examples": "node ./scripts/clear-examples.js",
    "format": "prettier --write src/**/*.{js,ts,css,scss,json,md}",
    "build-webpack": "webpack --mode production",
    "test-json-refs": "moca"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "json-refs": "^3.0.15",
    "mocha": "^10.2.0",
    "monaco-editor": "^0.38.0",
    "mustache": "^4.2.0",
    "node-red": "^3.0.2",
    "node-red-node-test-helper": "^0.3.1",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "should": "^13.2.3",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "uuid": "^8.3.2",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "jsonschema": "^1.4.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  }
}
